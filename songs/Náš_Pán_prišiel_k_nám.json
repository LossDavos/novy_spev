{
  "title": "Náš Pán prišiel k nám",
  "author": "",
  "categories": ["obetovanie"],
  "song_parts": [
    {
      "type": "verse",
      "lines": [
        "Náš Pán prišiel k nám v chlebe, víne.",
        "Oltár sa stal svätyňou.",
        "Tak poď k Nemu aj ty, On volá ťa.",
        "Tak pristúp sem s pokorou."
      ]
    }
  ]
}