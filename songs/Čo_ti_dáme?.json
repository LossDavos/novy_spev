  "song_parts": [
    {
      "type": "chorus",
      "lines": [
        "[D]Čo ti dáme, [h]dieťa lásky, [G]že si [A]prišlo [D]k nám?"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "[e]Skúšam prečítať [A]v tvojich očiach detských",
