{
  "title": "Magnificat",
  "author": "",
  "categories": [
    "mariánske"
  ],
  "song_parts": [
    {
      "type": "chorus",
      "lines": [
        "Magnificat, magnificat anima mea Dominum."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Velebí duša moja Pána",
        "a môj duch jasá v Bohu, mojom Spasiteľovi."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Lebo zhliadol na poníženosť svojej služobnice,",
        "hľa, od tejto chvíle blahoslaviť ma budú všetky pokolenia."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Veľké veci mi urobil ten, ktorý je mocný",
        "a sväté je Jeho meno."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Jeho milosrdenstvo je s z pokolenia na pokolenie",
        "s tými, čo sa ho boja."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Ukázal silu svojho ramena,",
        "rozptýlil tých, čo v srdci pyšne zmýšľajú."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Mocnárov zosadil z trónov a povýšil ponížených."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Hladných nakŕmil dobrotami",
        "a bohatých prepustil naprázdno."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Ujal sa Izraela, svojho služobníka,",
        "lebo pamätal na svoje milosrdenstvo."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Ako sľúbil našim otcom,",
        "Abrahámovi a jeho potomstvu naveky. Amen."
      ]
    }
  ]
}