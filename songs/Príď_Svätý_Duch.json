  "song_parts": [
    {
      "type": "chorus",
      "lines": [
        "Príď Svätý Duch, vojdi do nás.",
        "Príď Svätý Duch, plápolaj v nás.",
        "Príď Svätý Duch, oživuj nás,",
        "Duchu Svätý, príď."
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Príď, Duchu, prenikni nás,",
        "príď, Duchu, zastav sa pri nás.",
