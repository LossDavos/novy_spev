{
  "title": "Ježiško maličký, vložený v jasličky",
  "author": "",
  "categories": [
    "cezročné"
  ],
  "song_parts": [
    {
      "type": "verse",
      "lines": [
        "Ježiško maličký, vložený v jasličky,",
        "/:zima mu je, nad ním vzdychá to srdce Matičky.:/"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Chudobná jeho Mať, nemá ho čím odiať.",
        "/:Do vlastnej šatky odiala neba, zeme Pána./"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Nemá kolísočky, ani len podušky,",
        "/:na senku si sladko drieme, Ježiško maličký.:/"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Ležíš na tom sene, Kráľu neba, zeme,",
        "/:ako obetný Baránok za naše spasenie.:/"
      ]
    }
  ]
}