{
  "title": "Ó, Ježiš drahý",
  "author": "",
  "categories": [
    "cezročné"
  ],
  "song_parts": [
    {
      "type": "verse",
      "lines": [
        "Ó, Ježiš drahý, môj Boh a Pán,",
        "ja svoje srdce len Tebe dám.",
        "/:Ty si môj Pán, Ty si môj Kráľ,",
        "ja Teba milujem a uznávam.:/"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Ó, Priateľ dietok, tu prebývaš,",
        "tu lásku svoju nám rozdávaš.",
        "/:Budem vždy viac hriechu sa báť",
        "a Teba si v srdci opatrovať.:/"
      ]
    },
    {
      "type": "verse",
      "lines": [
        "Ja prosím Teba, môj Ježišu,",
        "opatruj mi moju dušu,",
        "/:nech nezblúdim a nezradím",
        "a Teba, môj Ježiš, neopustím.:/"
      ]
    }
  ]
}